$.ajax({
url:"http://localhost:3000/courses/addcourse",
data:{
courseName:"youku",
place:"home",
day:1,
schoolId:16,
reason:"damn it",
teacher:"dump ass"
},
type:"POST",
success:function(data){
console.log("success",data)
},
error:function(e){
console.log(e.responseText)
}

});



